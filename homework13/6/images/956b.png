            this.callbackOnClose = callback
        },
        executeCloseCallback() {
            if (typeof this.callbackOnClose === 'function') {
                this.callbackOnClose()
            }
        },
        setOptions(options) {
            this.options = options
        },
        show(callbackOnAccept, callbackOnDecline) {
            if (typeof callbackOnAccept === "function") {
                this.el.find('[data-submit]')
                    .off('click')
                    .on('click', callbackOnAccept)
            }

            if (typeof callbackOnDecline === "function") {
                $(document).off('hide.bs.modal')
                    .on('hide.bs.modal', callbackOnDecline)
            }

            // –ù—É–∂–Ω–æ –ª–∏ —Å–∫—Ä—ã–≤–∞—Ç—å –∫–Ω–æ–ø–∫—É submit
            if (this.options.hideSubmitButton) {
                this.el.find('[data-submit]').hide();
            } else {
                this.el.find('[data-submit]').show();
            }

            // –ù—É–∂–Ω–æ –ª–∏ —Å–∫—Ä—ã–≤–∞—Ç—å –∫–Ω–æ–ø–∫—É –∑–∞–∫—Ä—ã—Ç–∏—è
            if (this.options.hideCloseButton) {
                this.el.find('[data-close]').hide();
            } else {
                this.el.find('[data-close]').show();
            }

            let options = {
                show: true,
                ...this.options
            }
            this.el.modal(options)
        },
        hide() {
            this.el.modal('hide')
        }
    }
